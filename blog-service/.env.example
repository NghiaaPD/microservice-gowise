# Database Configuration
DB_URL=jdbc:postgresql://localhost:5432/blog_service
DB_USERNAME=postgres
DB_PASSWORD=your_password_here

# Server Configuration
SERVER_PORT=9101

# Eureka Configuration
EUREKA_SERVER_URL=http://localhost:8761/eureka/

# JWT Configuration (Generate your own secret key)
JWT_SECRET=your_base64_secret_key_here
JWT_TTL_SECONDS=600

# CORS Configuration
CORS_ALLOWED_ORIGINS=http://localhost:5173,http://your-domain:8081

# Security Configuration
FALLBACK_HEADER_AUTH=true

# JPA Configuration
JPA_DDL_AUTO=update
JPA_SHOW_SQL=true
JPA_FORMAT_SQL=true
